	@$(NORMAL_UNINSTALL)
	@list='$(ocamlstublib_DATA)'; for p in $$list; do \
	  f=$(am__strip_dir) \
	  echo " rm -f '$(DESTDIR)$(ocamlstublibdir)/$$f'"; \
	  rm -f "$(DESTDIR)$(ocamlstublibdir)/$$f"; \
	done
