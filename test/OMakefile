#(* $Id: OMakefile,v 1.1.2.2 2010/05/13 13:37:15 furuse Exp $ *)

.PHONY: all clean

FILES[]=
  test

OCAMLPACKS[]=
  graphics

SUB_PACKAGES[]= $(ALL_FILE_FORMATS) graphics freetype

BuildWithCamlimages($(SUB_PACKAGES))

.DEFAULT: $(OCamlProgram test, $(FILES))

clean:
  rm -f $(filter-proper-targets $(ls R, .))
  rm -f out.image
